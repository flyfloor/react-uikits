{
  "name": "react-uikits",
  "version": "2.0.0",
  "description": "react uikit collection",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all --parallel babel watch",
    "babel": "babel --presets es2015,react --watch src/component/ --out-dir lib/",
    "watch": "cross-env NODE_ENV=development node server.js",
    "build": "NODE_ENV=production webpack --config=webpack.production.js -p",
    "build-development": "NODE_ENV=development webpack --config=webpack.development.js",
    "lint": "eslint lib/"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "keywords": [
    "react",
    "css framework",
    "component",
    "uikit"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "author": "jerryshew",
  "license": "MIT",
  "homepage": "https://github.com/jerryshew/react-uikits",
  "dependencies": {
    "async-validator": "^1.8.1",
    "immutability-helper": "^2.6.0",
    "prop-types": "^15.6.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-ui-component": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^3.1.3",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^3.0.1",
    "force-case-sensitivity-webpack-plugin": "^0.1.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.1.3",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-highlight": "github:jerryshew/react-highlight#react-v16-compiled",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transform-hmr": "^1.0.0",
    "style-loader": "^0.23.1",
    "uglify-loader": "^3.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
